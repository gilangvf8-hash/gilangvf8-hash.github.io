<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  <!-- Untuk halaman biasa -->
  {% for page in site.pages %}
    {% if page.url != "/sitemap.xml" %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
      <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
    </url>
    {% endif %}
  {% endfor %}

  <!-- Untuk postingan blog -->
    <url>
<loc>https://gilangvf8-hash.github.io/post/post1.html</loc>
<lastmod>2025-09-12T00:00:00+00:00</lastmod>
</url>
<url>
<loc>https://gilangvf8-hash.github.io/post/post2.html</loc>
<lastmod>2025-09-13T00:00:00+00:00</lastmod>
</url>
<url>
<loc>https://gilangvf8-hash.github.io/post/post3.html</loc>
<lastmod>2025-09-14T00:00:00+00:00</lastmod>
</url>
<url>
<loc>https://gilangvf8-hash.github.io/post/post4.html</loc>
     <lastmod>2025-09-15T00:00:00+00:00</lastmod>
    </url>
  {% for post in site.posts %}
    <url>
      <loc>{{ site.url }}{{ post.url }}</loc>
      <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    </url>
  {% endfor %}

</urlset>
